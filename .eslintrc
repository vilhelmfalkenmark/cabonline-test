{
	"extends": ["airbnb-base", "prettier", "prettier/react", "eslint:recommended", "plugin:react/recommended"],
	"parser": "babel-eslint",
	"plugins": ["react", "prettier", "import"],
	"env": {
		"es6": true,
		"node": true,
		"jest": true,
		"browser": true
	},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "detect"
		},
		"import/resolver": {
			"node": {
				"paths": ["src", ".", "./"]
			}
		}
	}
}
